extends mainlayout.pug

block extra-css
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/css/bootstrap-datepicker3.css', type='text/css')
block extra-js
    script(src='https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.min.js')
    script(src='js/datepicker.js')

block header
    header(class='masthead')

block main-content
    section(class='success' id='ourcars')
        div(class='container')
          include includes/datepicker
        h2(class='text-center') Our cars
        hr(class='star-light') fefefefe
        div(class='row')

        header(class='masthead')
            div(class='container')


block main-content
    section(class='success')
        div(class='container')
            include includes/datepicker


        section(class='success' id='aboutus')
            div(class='container')
                div(class='row')
                    div(class='col-lg-4 offset-lg-2')
                        h3  Discount 20% on midsommar!
                        p Rent a car at the lowest price!We always have a good deal on weekly rates on all categories
                    div(class='col-lg-4')
                        h3 Book 4 day package and save 1 day!
                        p The most flexible rental solution. We are flexible - give us a challenge!

        //Scroll to Top Button (Only visible on small and extra-small screen sizes)
        div(class='scroll-top hidden-lg-up')
            a(class='btn btn-primary page-scroll' href='#page-top')
                i(class='fa fa-chevron-up')
                i(class='') Car rental at discounted price
